<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports"
			  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			  xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports
                                  http://jasperreports.sourceforge.net/xsd/jasperreport.xsd"
			  name="zapisnikSaIspita"
			  pageWidth="595" pageHeight="842"
			  columnWidth="555" leftMargin="20" rightMargin="20"
			  topMargin="20" bottomMargin="20">

	<!-- PARAMETRI -->
	<parameter name="predmetNaziv" class="java.lang.String"/>
	<parameter name="ispitniRokNaziv" class="java.lang.String"/>
	<parameter name="datumIspita" class="java.lang.String"/>
	<parameter name="nastavnikImePrezime" class="java.lang.String"/>
	<parameter name="ukupnoPrijavljenih" class="java.lang.Integer"/>
	<parameter name="ukupnoPolozilo" class="java.lang.Integer"/>
	<parameter name="prosecnaOcena" class="java.lang.Double"/>

	<!-- FIELDS -->
	<field name="ime" class="java.lang.String"/>
	<field name="prezime" class="java.lang.String"/>
	<field name="studProgramOznaka" class="java.lang.String"/>
	<field name="brojIndeksa" class="java.lang.Integer"/>
	<field name="godinaUpisa" class="java.lang.Integer"/>
	<field name="predispitni" class="java.lang.Integer"/>
	<field name="ispitni" class="java.lang.Integer"/>
	<field name="ukupno" class="java.lang.Integer"/>

	<!-- TITLE -->
	<title>
		<band height="90">
			<staticText>
				<reportElement x="197" y="30" width="200" height="30"/>
				<textElement textAlignment="Center" verticalAlignment="Middle">
					<font size="14" isBold="true"/>
				</textElement>
				<text><![CDATA[ZAPISNIK SA ISPITA]]></text>
			</staticText>
		</band>
	</title>

	<!-- PAGE HEADER -->
	<pageHeader>
		<band height="60">
			<staticText>
				<reportElement x="15" y="5" width="80" height="20"/>
				<text><![CDATA[Predmet:]]></text>
			</staticText>
			<textField>
				<reportElement x="95" y="5" width="120" height="20"/>
				<textFieldExpression><![CDATA[$P{predmetNaziv}]]></textFieldExpression>
			</textField>

			<staticText>
				<reportElement x="220" y="5" width="80" height="20"/>
				<text><![CDATA[Ispitni rok:]]></text>
			</staticText>
			<textField>
				<reportElement x="300" y="5" width="120" height="20"/>
				<textFieldExpression><![CDATA[$P{ispitniRokNaziv}]]></textFieldExpression>
			</textField>

			<staticText>
				<reportElement x="15" y="30" width="80" height="20"/>
				<text><![CDATA[Datum:]]></text>
			</staticText>
			<textField>
				<reportElement x="95" y="30" width="120" height="20"/>
				<textFieldExpression><![CDATA[$P{datumIspita}]]></textFieldExpression>
			</textField>

			<staticText>
				<reportElement x="220" y="30" width="80" height="20"/>
				<text><![CDATA[Nastavnik:]]></text>
			</staticText>
			<textField>
				<reportElement x="300" y="30" width="150" height="20"/>
				<textFieldExpression><![CDATA[$P{nastavnikImePrezime}]]></textFieldExpression>
			</textField>
		</band>
	</pageHeader>

	<!-- COLUMN HEADER -->
	<columnHeader>
		<band height="30">
			<staticText>
				<reportElement x="0" y="0" width="40" height="20"/>
				<textElement><font isBold="true"/></textElement>
				<text><![CDATA[Broj]]></text>
			</staticText>
			<staticText>
				<reportElement x="60" y="0" width="100" height="20"/>
				<textElement><font isBold="true"/></textElement>
				<text><![CDATA[Indeks]]></text>
			</staticText>
			<staticText>
				<reportElement x="160" y="0" width="120" height="20"/>
				<textElement><font isBold="true"/></textElement>
				<text><![CDATA[Ime i prezime]]></text>
			</staticText>
			<staticText>
				<reportElement x="300" y="0" width="80" height="20"/>
				<textElement><font isBold="true"/></textElement>
				<text><![CDATA[Predispitni]]></text>
			</staticText>
			<staticText>
				<reportElement x="400" y="0" width="80" height="20"/>
				<textElement><font isBold="true"/></textElement>
				<text><![CDATA[Ispitni]]></text>
			</staticText>
			<staticText>
				<reportElement x="500" y="0" width="55" height="20"/>
				<textElement><font isBold="true"/></textElement>
				<text><![CDATA[Ukupno]]></text>
			</staticText>
		</band>
	</columnHeader>

	<!-- DETAIL -->
	<detail>
		<band height="25">
			<textField>
				<reportElement x="0" y="0" width="40" height="20"/>
				<textFieldExpression><![CDATA[$V{REPORT_COUNT}]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement x="60" y="0" width="100" height="20"/>
				<textFieldExpression><![CDATA[$F{studProgramOznaka} + $F{brojIndeksa} + "/" + $F{godinaUpisa}]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement x="160" y="0" width="140" height="20"/>
				<textFieldExpression><![CDATA[$F{ime} + " " + $F{prezime}]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement x="300" y="0" width="80" height="20"/>
				<textFieldExpression><![CDATA[$F{predispitni}]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement x="400" y="0" width="80" height="20"/>
				<textFieldExpression><![CDATA[$F{ispitni}]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement x="500" y="0" width="55" height="20"/>
				<textFieldExpression><![CDATA[$F{ukupno}]]></textFieldExpression>
			</textField>
		</band>
	</detail>
</jasperReport>